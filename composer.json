{
    "name": "radiantabyss/packages-auth",
    "description": "RadiantAbyss Package: Auth",
    "keywords": [
        "ra"
    ],
    "homepage": "https://packages.radiantabyss.com/auth",
    "license": "proprietary",
    "type": "library",
    "authors": [
        {
            "name": "RA",
            "email": "robert@radiantabyss.com",
            "role": "owner"
        }
    ],
    "require": {
        "php": "^7.1",
        "intervention/image": "2.7.2",
        "firebase/php-jwt": "5.5.1"
    },
    "autoload": {
        "psr-4": {
            "RA\\Auth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RA\\Auth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RA\\Auth\\AuthServiceProvider"
            ]
        }
    }
}
